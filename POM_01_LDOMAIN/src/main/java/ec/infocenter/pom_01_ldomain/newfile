/* Formatted on 31/08/2015 16:51:21 (QP5 v5.252.13127.32867) */
DROP TABLE v_cabe_detalle;

CREATE TABLE IMAGEN_LAB.v_cabe_detalle

AS
   SELECT '' || (IMALAB_DETALLE_COMPRA.ROWID) rid,
          IMALAB_CABECERA.ID,
          IMALAB_CABECERA.DATOS_RUC,
          IMALAB_CABECERA.CLIENTE,
          IMALAB_CABECERA.FECHA_FACTURA,
          IMALAB_CABECERA.NUMERO_FACTURA,
          IMALAB_DETALLE_COMPRA.REFERIDO,
          IMALAB_DETALLE_COMPRA.PRECIO_CATALOGO,
          IMALAB_DETALLE_COMPRA.CANTIDAD,
          IMALAB_DETALLE_COMPRA.PRECIO,
          IMALAB_DETALLE_COMPRA.ANULADO,
          IMALAB_DETALLE_COMPRA.OBSERVACION
     FROM IMAGEN_LAB.IMALAB_CABECERA
          INNER JOIN IMAGEN_LAB.IMALAB_DETALLE_COMPRA
             ON (IMALAB_CABECERA.ID = IMALAB_DETALLE_COMPRA.CABECERA)
/